{
    "name": "ferror/symfony-package",
    "license": "MIT",
    "description": "",
    "type": "library",
    "autoload" : {
        "psr-4" : {
            "Ferror\\SymfonyPackage\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Ferror\\SymfonyPackage\\" : "tests/"
        }
    },
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12",
        "phpunit/phpunit": "^9.5",
        "phpstan/phpstan-phpunit": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^9.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
